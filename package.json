{
  "name": "ai-fraud-detection-system",
  "version": "1.0.0",
  "description": "Nigeria-ready AI fraud detection system with real-time transaction monitoring",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "docker:build": "docker build -t fraud-detection-system .",
    "docker:run": "docker run -p 3000:3000 fraud-detection-system"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.10.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mongoose": "^7.5.0",
    "neo4j-driver": "^5.12.0",
    "node-fetch": "^3.3.2",
    "redis": "^4.6.10",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-rate-limit": "^6.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.19.13",
    "@types/supertest": "^2.0.15",
    "@types/winston": "^2.4.4",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
